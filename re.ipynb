    "import pandas as pd\n",
    "import re\n",
    "\n",
    "for num in range(20):\n",
    "    filename = f'jobkorea_data{num}.csv'\n",
    "    df = pd.read_csv(filename)\n",
    "    df['answer'] = df['answer'].apply(lambda x: re.sub(r'글자수+.*', ' ', str(x)))\n",
    "    df.to_csv(filename, index=False)\n"
